          RowBox[{"CouplingConstant", "\[Rule]", 
            SubscriptBox["g", "s"]}], ",", 
          RowBox[{"DummyIndex", "\[Rule]", "Automatic"}], ",", 
          RowBox[{"Explicit", "\[Rule]", "False"}], ",", 
          RowBox[{"PartialD", "\[Rule]", "RightPartialD"}], ",", 
          RowBox[{"QuantumField", "\[Rule]", "A"}]}], "}"}], 
