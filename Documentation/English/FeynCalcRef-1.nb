          RowBox[{"Factoring", "\[Rule]", "False"}], ",", 
          RowBox[{"FinalSubstitutions", "\[Rule]", 
            RowBox[{"{", 
              RowBox[{"DiracBasis", "\[Rule]", "Identity"}], "}"}]}]}], "}"}],
       TraditionalForm]], "Output",
  CellLabel->"Out[417]="]
