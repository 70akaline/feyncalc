Cell[TextData[{
  "ToHypergeometric[",
  Cell[BoxData[
      FormBox[
        RowBox[{
          SuperscriptBox[
            StyleBox["t",
              FontSlant->"Plain"], 
            StyleBox["b",
              FontSlant->"Plain"]], " ", 
          SuperscriptBox[
            RowBox[{"(", 
              RowBox[{"1", "-", 
                StyleBox["t",
                  FontSlant->"Plain"]}], ")"}], 
            StyleBox["c",
              FontSlant->"Plain"]], " ", 
          SuperscriptBox[
            RowBox[{"(", 
              RowBox[{"1", "+", 
                RowBox[{
                  StyleBox["t",
                    FontSlant->"Plain"], " ", 
                  StyleBox["z",
                    FontSlant->"Plain"]}]}], ")"}], 
            StyleBox["a",
              FontSlant->"Plain"]]}], TraditionalForm]],
    FontVariations->{"CompatibilityType"->0}],
  ", ",
  "t",
  "] returns ",
  Cell[BoxData[
      FormBox[
        RowBox[{
          RowBox[{"\[CapitalGamma]", "(", 
            RowBox[{"b", "+", "1"}], ")"}], " ", 
          RowBox[{
            RowBox[{"\[CapitalGamma]", "(", 
              RowBox[{"c", "+", "1"}], ")"}], "/", 
            RowBox[{"\[CapitalGamma]", "(", 
              RowBox[{"b", "+", "c", "+", "2"}], ")"}]}], " ", 
          FormBox[
            RowBox[{
              SubscriptBox["\[Null]", "2"], 
              RowBox[{
                SubscriptBox["F", "1"], "(", 
                RowBox[{
                  RowBox[{
                    RowBox[{"-", "a"}], ",", 
                    RowBox[{"b", "+", "1"}]}], ";", 
                  RowBox[{"b", "+", "c", "+", "2"}], ";", 
                  RowBox[{"-", "z"}]}], ")"}]}],
            "TraditionalForm"]}], TraditionalForm]]],
  ". Remember that Re ",
  StyleBox["b",
    FontSlant->"Italic"],
  " >0 and Re (",
  StyleBox["c",
    FontSlant->"Italic"],
  "-",
  StyleBox["b",
    FontSlant->"Italic"],
  ") > 0 should hold (need not be set in ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  ")."
}], "Text",
