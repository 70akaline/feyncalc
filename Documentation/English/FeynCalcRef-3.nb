Cell[TextData[{
  "TrickMandelstam[expr, {s, t, u, ",
  Cell[BoxData[
      FormBox[
        SubsuperscriptBox[
          StyleBox["m",
            FontSlant->"Plain"], "1", "2"], TraditionalForm]]],
  " + ",
  Cell[BoxData[
      FormBox[
        SubsuperscriptBox[
          StyleBox["m",
            FontSlant->"Plain"], "2", "2"], TraditionalForm]]],
  "+ ",
  Cell[BoxData[
