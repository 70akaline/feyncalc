          RowBox[{"\[CapitalGamma]", "(", 
            RowBox[{"b", "+", "1"}], ")"}], " ", 
          RowBox[{
            RowBox[{"\[CapitalGamma]", "(", 
              RowBox[{"c", "+", "1"}], ")"}], "/", 
            RowBox[{"\[CapitalGamma]", "(", 
              RowBox[{"b", "+", "c", "+", "2"}], ")"}]}], " ", 
